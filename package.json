{
  "name": "playwright-odiff",
  "version": "1.0.0",
  "scripts": {
    "test:local": "bash test-local.sh",
    "test:clean": "rm -rf screenshots/* && rm -rf public/* && rm -rf test-results/* && rm -rf playwright-report/*",
    "test:update-baseline": "playwright test --update-snapshots",
    "test:report": "node generate-report.js",

    "test:ci": "playwright test",
    "test:install": "playwright install --with-deps",
    "test:verify": "playwright --version && playwright browsers list",

    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@playwright/test": "^1.39.0",
    "odiff-bin": "^2.6.1"
  },
  "devDependencies": {
    "odiff-bin": "^3.1.2",
    "playwright": "^1.48.2"
  }
}